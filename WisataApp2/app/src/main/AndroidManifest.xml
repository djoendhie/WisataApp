<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kun.jun.wisataapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".gabung.loginloading.LoginActivity" />
        <activity android:name=".gabung.loginloading.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".gabung.rcview.IsiActivity"
            android:label="Destination"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".avtivity.DatabaseFirebaseActivity"
            android:label="Recomended"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".helper.MyFuction" />
        <activity
            android:name=".AboutActivity"
            android:label="About Me"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".gabung.RFire.MainMainActivity"
            android:label="Recomended"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>